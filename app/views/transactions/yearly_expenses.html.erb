    <div class='col-lg-12'>
      <div class='ibox float-e-margins'>

        <div class='ibox-content'>
          <div class='table-responsive'>

            <table class='table table-hover' >
              <thead>
                <tr>                  
                  <th class="col-sm-2">CATEGORY</th>
